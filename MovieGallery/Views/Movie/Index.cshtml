@model IEnumerable<MovieGallery.Models.Movie>

<h2>Movies</h2>

<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Genre</th>
            <th>Release Date</th>
            <!-- Add additional columns if needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model)
        {
            <tr>
                <td>@movie.Title</td>
                <td>@movie.Genre</td>
                <td>@movie.ReleaseDate</td>
                <!-- Add additional columns if needed -->
            </tr>
        }
    </tbody>
</table>